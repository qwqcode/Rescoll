{
  "productName": "Rescoll",
  "appId": "com.qwqaq.rescoll",
  "directories": {
    "output": "build"
  },
  "files": [
    {
      "from": "dist",
      "to": "dist",
      "filter": ["**/*"]
    },
    {
      "from": ".",
      "filter": ["package.json"]
    }
  ],
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 150,
        "type": "file"
      }
    ]
  },
  "mac": {
    "icon": "assets/app-icons/icon.icns"
  },
  "win": {
    "icon": "assets/app-icons/icon.ico"
  },
  "linux": {
    "icon": "assets/app-icons"
  }
}
